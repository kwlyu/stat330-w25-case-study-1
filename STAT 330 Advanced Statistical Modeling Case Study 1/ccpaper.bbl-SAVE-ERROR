      \strng{namehash}{d637b960402957c19f7a184e0b355543}
      \strng{fullhash}{0bb6a8475b52ad6d8e2722a6a7ff32d8}
      \strng{bibnamehash}{0bb6a8475b52ad6d8e2722a6a7ff32d8}
