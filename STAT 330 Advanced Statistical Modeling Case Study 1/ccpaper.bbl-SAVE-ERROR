      \strng{namehash}{38b0d6ffefe9c0115de90c6397d3449b}
      \strng{fullhash}{38b0d6ffefe9c0115de90c6397d3449b}
      \strng{bibnamehash}{38b0d6ffefe9c0115de90c6397d3449b}
